<mvc:View
		xmlns:core="sap.ui.core"
          xmlns:mvc="sap.ui.core.mvc" 
          xmlns:table="sap.ui.table"
          xmlns="sap.m" 
          xmlns:semantic="sap.m.semantic" 
          controllerName="com.sap.appcelima.controller.BaseController"
          xmlns:l="sap.ui.layout"
	      xmlns:form="sap.ui.layout.form">

		<Page id="Login" navButtonPress="onNavBack" showNavButton="true" title="{Text02}" busy="{detailView>/busy}"
			busyIndicatorDelay="{detailView>/delay}">
			<content>
				<form:SimpleForm editable="true" id="__form0" width="auto" maxContainerCols="3" layout="ResponsiveGridLayout" labelSpanL="2" labelSpanM="2"
					emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
					<form:title>
						<core:Title></core:Title>
					</form:title>
					<FlexBox width="100%" id="__box0" direction="Column" alignItems="Center" justifyContent="SpaceAround">
						<items>
							<Image width="900px" height="450px" id="__image0" src="imagen/Portada.png" alt="alttextimage" visible="true" backgroundPosition="-74px -14px"/>
						</items>
					</FlexBox>
					<form:title>
						<core:Title></core:Title>
					</form:title>
				</form:SimpleForm>
			</content>
		</Page>
	
</mvc:View>